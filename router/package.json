{
  "name": "@lopatnov/namespace-router",
  "version": "2.0.0",
  "license": "Apache-2.0",
  "author": "lopatnov",
  "description": "SPA router with lazy loading for @lopatnov/namespace. History & hash modes, dynamic imports, route params.",
  "keywords": ["router", "spa", "lazy-loading", "namespace", "tree-shakeable", "ES2024"],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "peerDependencies": {
    "@lopatnov/namespace": "^2.0.0"
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@lopatnov/namespace": "file:../namespace",
    "jsdom": "^28.1.0",
    "tsdown": "latest",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
